
	on init
		do ( A, toto )
		do ( A, titi )
		do ( A, tata )
		do ( A, tutu )
		do ( A, zebulon )
	else on A
		/* %(A,?) results should not be in any particular order
		   but in this mono-thread implementation, they will be
		   in order reverse to creation order */
		do (( B, ... ), %(A,?) )
	else
		do >: %( B, ?:... )
		do exit

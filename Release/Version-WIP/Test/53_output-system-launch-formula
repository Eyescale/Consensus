	do : cosystem : !! Cosystem(
		titi, toto
		?:(( ., /(ON|OFF)/ ), ^^ ) { ( %?::(?,.), *^^ ) |
			?:( ., %? ) { ( %?::(?,.), %| ) | {
				?:( ?, %?::((?,.),.))
					(( %?::(?,.), *^%?::(.,?) ), %|::((?,.),.)),
				?:( ?, %?::((.,?),.))
					(( %?::(?,.), *^%?::(.,?) ), %|::((.,?),.)),

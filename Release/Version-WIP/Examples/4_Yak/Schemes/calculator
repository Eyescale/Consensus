
	(( Rule, base ), ( Schema, {
		(: %operation :)
	} ))
	(( Rule, operation ), ( Schema, {
		(:%multiplication:)
		(:%addition:)
	} ))
	(( Rule, multiplication ), ( Schema, {
		(:%number * %number:)
		(:%multiplication * %number:)
	} ))
	(( Rule, addition ), ( Schema, {
		(:%number + %number:)
		(:%addition + %number:)
		(:%multiplication + %number:)
		(:%number + %multiplication :)
		(:%multiplication + %multiplication :)
	} ))
	(( Rule, number ), ( Schema, (:\i:) ))
